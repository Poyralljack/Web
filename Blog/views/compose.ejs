<%- include("partials/header") -%>
<h1>Compose</h1>
<form class="item" action="/compose" method="post">
    <div class="form-group">
        <label >Title</label>
        <textarea class="form-control" name="postTitle" cols="30" rows="1"></textarea>
        <label >Post</label>
        <textarea class="form-control" name="postBody" cols="30" rows="5"></textarea>
      </div>
      <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>
<%- include("partials/footer") -%>